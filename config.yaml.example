telegram:
  bot_token: "your-telegram-bot-token"

openai:
  api_key: "your-openai-api-key"
  model: "gpt-4.1-nano"

auth:
  allowed_chat_ids:
    - 123456789
    - 987654321

logging:
  level: "info"  # debug, info, warn, error
  file: "telegpt.log"  # log file path, leave empty to disable file logging
  console: true  # log to console
